@using Sandbox.SOA.Portal.Helpers
@model Sandbox.SOA.Portal.Models.People.PersonEditViewModel

@{
    ViewBag.Title = "Person Edit";
}

@section breadcrumb{

    @(Html.Breadcrumb()
          .Add("Home", Url.Home())
          .Add("People", Url.People())
          .Add("Edit", Url.PersonEdit(Model.Identifier))
          .Render())
}

@Html.Partial("EditForm")
