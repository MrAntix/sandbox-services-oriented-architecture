@using Sandbox.SOA.Portal.Helpers
@model Sandbox.SOA.Portal.Models.People.PersonDeleteViewModel

@{
    ViewBag.Title = "Person Delete";
}

@section breadcrumb{

    @(Html.Breadcrumb()
          .Add("Home", Url.Home())
          .Add("People", Url.People())
          .Add("Delete", Url.PersonDelete(Model.Identifier))
          .Render())
}

@Html.Partial("DeleteForm")
