@model Sandbox.SOA.Common.Contracts.People.Addresses.PersonAddressSearchResult

@{
    ViewBag.Title = "Index";
}

@section breadcrumb{

    @(Html.Breadcrumb()
          .Add("Home", Url.Home())
          .Add("People", Url.People())
          .Add("Edit", Url.PersonEdit(Model.Person.Identifier))
          .Add("Addresses", Url.PersonAddresses(Model.Person.Identifier))
          .Render())
}


